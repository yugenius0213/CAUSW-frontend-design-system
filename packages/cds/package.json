{
  "name": "@causw/design-system",
  "version": "0.0.4",
  "description": "CAUSW Design System - Complete design system for CAU Software Community Service",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@causw/core": "workspace:*",
    "@causw/tokens": "workspace:*",
    "@causw/components": "workspace:*",
    "@causw/icons": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CAUCSE/CAUSW-frontend-design-system",
    "directory": "packages/cds"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
